        "jest": "^26.6.3",
        "prettier": "2.8.8",
        "start-server-and-test": "^2.0.0",
        "ts-loader": "9.5.0",
        "ts-node": "10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "4.9.5",
        "webpack": "^5.51.1",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.15.1"
    },
    "dependencies": {
        "react": "^18.0.0",
