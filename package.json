    "license": "MIT",
    "scripts": {
        "build": "webpack --mode=production --env production --progress",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "dev": "webpack-cli serve --mode=development --env development --open --hot",
        "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
        "lint:fix": "eslint './src/**/*.{ts,tsx,js,jsx}' --fix",
        "test:ci": "start-server-and-test dev tcp::8080 cy:run",
        "test:setup": "mkdir -p .nyc_output",
        "test": "yarn test:setup && yarn test:ci && nyc report --reporter=lcov --reporter=json"
    },
    "resolutions": {
        "jackspeak": "2.1.1"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.8.1",
